{
  "name": "tob-web",
  "version": "0.0.1",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "node preinstall.js && ng serve -o --base-href ${NG_BASE_HREF:-/} --deploy-url ${NG_DEPLOY_URL:-/} --env ${NG_ENV:-dev}",
    "build": "ng build --prod --base-href ${NG_BASE_HREF:-/} --deploy-url ${NG_DEPLOY_URL:-/} ${NG_BUILD_FLAGS:-}",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "preinstall": "node preinstall.js",
    "postinstall": "ng build --prod --base-href ${NG_BASE_HREF:-/} --deploy-url ${NG_DEPLOY_URL:-/} ${NG_BUILD_FLAGS:-} && cp dist/themes/_active/index.html dist/index.html"
  },
  "dependencies": {
    "@angular/common": "~4.4.0",
    "@angular/compiler": "~4.4.0",
    "@angular/compiler-cli": "~4.4.0",
    "@angular/core": "~4.4.0",
    "@angular/forms": "~4.4.0",
    "@angular/http": "~4.4.0",
    "@angular/platform-browser": "~4.4.0",
    "@angular/platform-browser-dynamic": "~4.4.0",
    "@angular/router": "~4.4.0",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.6",
    "@ngx-translate/core": "~8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "bootstrap-sass": "~3.3.7",
    "compare-versions": "~3.1.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "localize-router": "~1.0.0-rc.4",
    "localize-router-http-loader": "~1.0.0",
    "mygovbc-bootstrap-theme": "^0.3.0",
    "rxjs": "^5.4.1",
    "typescript": ">=2.1.0 <2.4.0",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "~1.6.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngx-bootstrap": "^1.7.1",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0"
  }
}
